{
	"_version": "1.15.0",
	"sap.app": {
		"id": "winnings.com.inbound_delivery_ic",
		"type": "card",
		"title": "Inbound Deliveries",
		"subTitle": "Inbound Deliveries Integration Card",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
        "deviceTypes": {"desktop": true, "phone": true, "tablet": true}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "winnings.com.inbound_delivery_ic.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.38",
			"libs": {
				"sap.m": {}
			}
		},
		"resources" : {
			"css" : [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing" : {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "winnings.com.inbound_delivery_ic",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "deliveries",
					"target": "deliveries"
			 	},
				{
					"pattern": "detail",
					"name": "detail",
					"target": "detail"
				}
			],
			 "targets": {
				"deliveries": {
					"id": "deliveries",
					"name": "Deliveries",
					"level" : 1
				},
				"detail": {
					"id": "detail",
					"name": "Detail",
					"level" : 1
				}
			 }
		}
	},
	"sap.card": {
		"type": "Component",
        "designtime": "dt/configuration",
		"configuration": {
			"destinations": {
				"ds4": {
					"name": "ds4"
				}
			}
		}
	},
    "sap.platform.mobilecards": {
        "compatible": false
    }
}